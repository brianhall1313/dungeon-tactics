; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="dungeon tactics"
run/main_scene="res://Scene/start_screen.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://textures/icon.svg"

[autoload]

Global="*res://Global/Global.gd"
GlobalSignalBus="*res://Global/global_signal_bus.gd"
GlobalInput="*res://Global/input.gd"
Armor="*res://Resources/armor.gd"
Weapons="*res://Resources/weapons.gd"
ClassData="*res://Resources/class_data.gd"
RangeFinder="*res://Resources/range_finder.gd"
MovementTools="*res://Resources/movement_tools.gd"
AttackTools="*res://Resources/attack_tools.gd"
SpellsAndAbilities="*res://Resources/spells_and_abilities.gd"
Names="*res://Resources/names.gd"
LevelData="*res://Resources/level_data.gd"
SaveAndLoad="*res://Resources/save_and_load.gd"
World="*res://Global/World.gd"
Animator="*res://Resources/animator.gd"

[display]

window/stretch/mode="viewport"

[filesystem]

import/blender/enabled=false

[input]

Escape={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":4194305,"key_label":0,"unicode":0,"echo":false,"script":null)
]
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.180005, 0.180005, 0.180005, 1)
